{
    "name": "Robot_Pi_4WD_ROS",
    "dockerComposeFile": "../docker-compose.yaml",
    "service": "robot",
    "workspaceFolder": "/workspaces/Robot_Pi_4WD_ROS",
    "overrideCommand": false,
    "containerEnv": {
      "UDEV": "1"
    },
    "remoteEnv": {
      "UDEV": "1"
    },
    "updateContentCommand": "bash .devcontainer/post_creation_script.sh",
    // "postCreateCommand": [
    //   "/usr/bin/supervisord"
    // ],
    "postStartCommand" : "/usr/bin/supervisord",
    "customizations": {
      "vscode": {
        "extensions": [
            "ms-python.python",
            "ms-vscode.cpptools-extension-pack",
            "ms-azuretools.vscode-docker",
            "ms-vscode.cmake-tools",
            "twxs.cmake",
            "webfreak.debug"
        ]
      }
    }
  }